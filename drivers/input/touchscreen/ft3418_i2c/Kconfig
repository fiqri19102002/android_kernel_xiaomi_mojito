#
# Focaltech Touchscreen driver configuration
#

config TOUCHSCREEN_FT3418_I2C
    bool "Focaltech Touchscreen for mojito"
    depends on DRM
    default y
    help
      Say Y here if you have Focaltech touch panel.
      If unsure, say N.
      
config TOUCHSCREEN_FT3418_I2C_DIRECTORY
    string "Focaltech ts directory name"
    default "focaltech_touch_ft3418_i2c"
    depends on TOUCHSCREEN_FT3418_I2C
    
